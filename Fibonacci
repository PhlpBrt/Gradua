package recursividade;

public class Fibonacci {

    public static int fibo(int n) {
        if (n < 2) {
            return 1;
        } else {

            return fibo(n - 1) + fibo(n - 2);
        }
    }

    public static void main(String[] args) {
        System.out.print("NumÃ©ros de Fibonacci : ");
        for (int i = 0; i < 10; i++) {
            System.out.print(fibo(i) + " ");
        }
    }
}
